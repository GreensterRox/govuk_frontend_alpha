{% set compHandle = entity.component().handle %}

<div class="Browser-panel Browser-code Browser-nunjucks" data-role="tab-panel" id="browser-{{ entity.id }}-panel-nunjucks">
  <div class="Prose Prose--condensed">
    <h3>To call this component using Nunjucks:</h3>
    <code class="Code Code--lang-njk">
      <pre contenteditable >{{ "{{ " + (entity.getResolvedContext() | async | generateComponentInvocation('njk', compHandle)) + " }}" | highlight('js') }}</pre>
    </code>
  </div>
</div>
